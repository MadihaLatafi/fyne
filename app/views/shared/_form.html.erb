<%= simple_form_for(@event) do |f| %>
<%= f.input :name %>
<%= f.input :category %>
<%= f.input :subcategory %>
<%= f.input :start_time %>
<%= f.input :end_time %>
<%= f.input :street_address %>
<%= f.input :zip_code %>
<%= f.input :city %>
<%= f.input :state %>
<%= f.input :country %>
<%= f.input :photo %>
<%= f.submit "Save changes", class: "btn btn-primary" %>
<% end %>
